using namespace std;

int main()
{
    int x, y, max;
    cin >> x >> y;  // Ввод двух чисел

    (x > y) ? cout << x : cout << y << endl; //1 Тернарный оператор: вывод большего числа

    max = (x > y) ? x : y;                   //2 Тернарный оператор: присваивание большего числа переменной max

    cout << max << endl; // Вывод значения переменной max

    return 0;
}